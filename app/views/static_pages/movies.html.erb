    <div class="panel panel-info">     
    
      <div class="panel-heading"> <!-- arterial lines accordion-->
        <h3 class="panel-title">
          <a href='#arterial' class="accordion-toggle" data-toggle="collapse" data-parent="#accordion">
            Arterial lines
          </a>
        </h3>
      </div>
    
      <div id="arterial" class="panel-collapse collapse"> 
